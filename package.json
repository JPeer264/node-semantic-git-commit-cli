{
  "name": "semantic-git-commit-cli",
  "version": "3.7.0",
  "description": "A CLI for semantic git commits",
  "main": "dest",
  "bin": {
    "semantic-git-commit": "./dest/index.js",
    "sgc": "./dest/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage --runInBand",
    "lint": "eslint lib __tests__",
    "prepublish": "npm run build",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "sgc": "ts-node lib/index.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JPeer264/node-semantic-git-commit-cli.git"
  },
  "author": "Jan Peer St√∂cklmair <janpeer264@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JPeer264/node-semantic-git-commit-cli/issues"
  },
  "keywords": [
    "semantic",
    "git",
    "commits",
    "cli",
    "fast",
    "enhance",
    "workflow"
  ],
  "homepage": "https://github.com/JPeer264/node-semantic-git-commit-cli#readme",
  "devDependencies": {
    "@types/findup-sync": "^4.0.2",
    "@types/jest": "^28.1.8",
    "@types/lodash.merge": "^4.6.7",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "randomstring": "^1.2.2",
    "sinon": "^14.0.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "execa": "^5.1.1",
    "findup-sync": "^5.0.0",
    "fs-extra": "^10.1.0",
    "git-commit-count": "^1.1.3",
    "git-commit-range": "^1.2.0",
    "inquirer": "^8.2.4",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "is-git-added": "^1.0.2",
    "is-git-repository": "^2.0.0",
    "json-extra": "^2.0.1",
    "lodash.merge": "^4.6.2",
