{
  "name": "semantic-git-commit-cli",
  "version": "3.7.0",
  "description": "A CLI for semantic git commits",
  "main": "dest",
  "bin": {
    "semantic-git-commit": "./dest/index.js",
    "sgc": "./dest/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage --runInBand",
    "lint": "eslint lib __tests__",
    "prepublish": "npm run build",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "sgc": "ts-node lib/index.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JPeer264/node-semantic-git-commit-cli.git"
  },
  "author": "Jan Peer St√∂cklmair <janpeer264@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JPeer264/node-semantic-git-commit-cli/issues"
  },
  "keywords": [
    "semantic",
    "git",
    "commits",
    "cli",
    "fast",
    "enhance",
    "workflow"
  ],
  "homepage": "https://github.com/JPeer264/node-semantic-git-commit-cli#readme",
  "devDependencies": {
    "@types/findup-sync": "^2.0.2",
    "@types/jest": "^25.2.3",
    "@types/lodash.merge": "^4.6.6",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "coveralls": "^2.12.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^3.0.5",
    "jest": "^25.1.0",
    "lint-staged": "^9.4.0",
    "randomstring": "^1.1.5",
    "sinon": "^7.1.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "execa": "^0.6.1",
    "findup-sync": "^4.0.0",
    "fs-extra": "^7.0.0",
    "git-commit-count": "^1.1.2",
    "git-commit-range": "^1.2.0",
    "inquirer": "^7.1.0",
    "inquirer-autocomplete-prompt": "^1.0.2",
    "is-git-added": "^1.0.1",
    "is-git-repository": "^1.0.1",
    "json-extra": "^2.0.1",
    "lodash.merge": "^4.6.0",
    "object.entries": "^1.0.4",
    "path-is-absolute": "^2.0.0",
    "temp-dir": "^1.0.0",
    "update-notifier": "^2.1.0",
    "yargs": "^15.3.1"
  }
}
